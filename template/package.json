    "@hickory/browser": "^1.0.0-beta.7",<% } %>
    "loadable-components": "2.2.2",<% if (pwa) { %>
    "offline-plugin": "^5.0.3",<% } %>
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "@poi/plugin-bundle-report": "^10.0.1",
    "@poi/plugin-eslint": "^10.0.2",<% if (pwa) { %>
    "@poi/plugin-offline": "^10.0.1",<% } %>
    "babel-eslint": "^8.2.3",
    "babel-preset-poi": "^10.0.1",
    "eslint": "^4.19.1",
